# Copy to .env and add your keys. .env is gitignored.

# OpenAI — used for embeddings (RAG), classification, discovery, chat
OPENAI_API_KEY=sk-proj-your-key-here

# Anthropic — used for code generation (Claude Sonnet). Falls back to GPT-4o if missing.
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Use LangGraph multi-agent system (4 agents, 6 tools)
USE_LANGGRAPH=true
